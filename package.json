{
  "name": "medialab-website-api",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently --names api,admin,site -c blue,green,yellow \"npm run dev:api\" \"npm run dev:admin\" \"npm run dev:site\"",
    "dev:api": "nodemon api/server.js",
    "dev:admin": "cd admin && npm run dev",
    "dev:site": "cd site && npm run dev"
  },
  "contributors": [
    {
      "name": "Guillaume Plique",
      "url": "http://github.com/Yomguithereal"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "config": "^2.0.1",
    "express": "^4.16.3",
    "fs-extra": "^7.0.0",
    "json-server": "^0.14.0"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.4"
  },
  "nodemonConfig": {
    "ignore": [
      "admin/*",
      "data/*",
      "website/*"
    ]
  }
}
